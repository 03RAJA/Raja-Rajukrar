import java.util.*;
class ArraylistDemo1
{
   public static void main(String[] args)
   {
   ArrayList al=new ArrayList();
   al.add("Raja");                                            //Default capacity=10
   al.add(30);                                               //New Capacity=(cc*3/2)+1
   al.add(null);
   al.add("Raja"); 
   System.out.println(al.remove(1));
   System.out.println(al);
   al.set(0,"Rahul"); 
   System.out.println(al);                                     
   al.add(1,"Ram");
   System.out.println(al);
   al.add(0,"Akshay"); 
   System.out.println(al);
   }
