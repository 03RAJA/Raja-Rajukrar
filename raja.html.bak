import java.util.*;
class LinkedListDemo
{
   public static void main(String[] args)
   {
   LinkedList l1=new LinkedList();
   l1.add("Raja");
   l1.add(30);
   l1.add(null);
   l1.add("Raja"); 
   System.out.println(l1);
   l1.set(0,"Rahul");                              //set method work is replace 
   System.out.println(l1);                                     
   l1.add(1,"Ram");
   System.out.println(l1);                                    
   l1.add(0,"Akshay");
   System.out.println(l1);                                     
   l1.removeLast();
   System.out.println(l1);                                              
   l1.addFirst("Hero");                                            
   System.out.println(l1);
   }
}
